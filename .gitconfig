[color]
	diff = auto
	status = auto
	branch = auto
	commit = auto
	interactive = auto
	showbranch = auto
	pager = true
	ui = true
[color "status"]
	added = green
	changed = red bold
	untracked = magenta bold
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[core]
	whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = ~/.gitignore-rab
[apply]
	whitespace = nowarn
[branch]
	autosetuprebase = always
[rerere]
	enabled = 1
